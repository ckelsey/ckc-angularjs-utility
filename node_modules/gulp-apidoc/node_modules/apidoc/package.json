  },
  "devDependencies": {
    "apidoc-example": "*",
    "jshint": "^2.5.10",
    "mocha": "~2.1.0",
    "semver": "^4.1.0",
    "should": "~4.6.0"
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": false,
    "eqeqeq": true,
    "forin": true,
    "latedef": false,
    "newcap": true,
    "undef": true,
    "unused": true,
    "trailing": true,
    "node": true,
    "browser": true,
    "predef": [
      "it",
      "describe",
